{
  "name": "symfona/pagination",
  "type": "library",
  "description": "Pagination Library",
  "keywords": ["php", "pagination"],
  "homepage": "https://github.com/symfona/pagination",
  "license": "MIT",
  "authors": [
    {
      "name": "Sofy Korzhova",
      "email": "sofykorzhova@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Symfona\\Pagination\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Symfona\\Pagination\\Tests\\": "tests"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  }
}
